      operation 179242 "print"
	cpp_virtual cpp_inline public explicit_return_type "size_t"
	nparams 2
	  param in name "c" explicit_type "char"
	  param in name "nl" explicit_type "bool"
	    defaultvalue "false"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}${t0} ${p0}${v0}, ${t1} ${p1}${v1}${)}${const}${volatile}${throw}${abstract} {
		// Scrivo direttamente il byte
		size_t n = write(static_cast<uint8_t>(c));
		if (nl) {
			// Aggiungo CR+LF
			n += write(reinterpret_cast<const uint8_t*>(\"\\r\\n\"), 2);
		}
		return n;
	};
"
	
	
	
	
      end

      operation 179370 "print"
	cpp_virtual cpp_inline public explicit_return_type "size_t"
	nparams 2
	  param in name "x" explicit_type "int32_t"
	  param in name "nl" explicit_type "bool"
