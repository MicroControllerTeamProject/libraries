      operation 179114 "print"
	cpp_virtual cpp_inline public explicit_return_type "size_t"
	nparams 2
	  param in name "s" explicit_type "char"
	  param in name "nl" explicit_type "bool"
	    defaultvalue "false"
	cpp_decl "    ${comment}${friend}${static}${inline}${virtual}${typeprefix}${type} ${name}${(}const ${t0} * ${p0}${v0}, ${t1} ${p1}${v1}${)}${const}${volatile}${throw}${abstract} {
		size_t n = write(reinterpret_cast<const uint8_t*>(s), strlen(s));
		if (nl) {
			n += write(reinterpret_cast<const uint8_t*>(\"\\r\\n\"), 2);
		}
		return n;
	};
"
	
	
	
	
	comment " --- print/println wrappers basati su write() ---
/ Stampa una stringa, con newline opzionale"
